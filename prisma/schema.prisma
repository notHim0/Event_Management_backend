generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}


model Event {
    id String @id @default(uuid())
    name String
    // rsvps User[]
    type String
    venue String
    club_organisers String?
    members String?
    description String
    thumbnail String?
    timestamp DateTime?
}